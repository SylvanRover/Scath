            if (keyboard_check_released (ord('W')))
            {
                sprite_index = spr_player_attack_down;
            }
            if (keyboard_check_released (ord('A')))
            {
                sprite_index = spr_player_attack_down;
            }
            if (keyboard_check_released (ord('S')))
            {
                sprite_index = spr_player_attack_down;
            }
            if (keyboard_check_released (ord('D')))
            {
                sprite_index = spr_player_attack_down;
            }
        }
    }
}
