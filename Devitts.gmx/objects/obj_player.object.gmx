                sprite_index = spr_player_attack_right;
            }
            if (keyboard_check_released (ord('D')))
            {
                sprite_index = spr_player_attack_right;
            }
        }
    }
    
    if (direction &gt;= 45 &amp;&amp; direction &lt;= 135) // Up
    {
        if (combatAxisObject) 
