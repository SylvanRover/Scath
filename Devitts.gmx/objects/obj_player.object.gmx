{               
    // Melee Attack
    if (direction &gt;= 0 &amp;&amp; direction &lt; 45 || direction &gt; 315) // Right
    {
        if (combatAxisObject) 
        {
            if (keyboard_check_released (ord('W')))
            {
                sprite_index = spr_player_attack_right;
            }
            if (keyboard_check_released (ord('A')))
