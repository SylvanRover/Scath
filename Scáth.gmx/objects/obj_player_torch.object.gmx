            <string>/// Turn on/off light
wait = false;

if (lightOn &amp;&amp; mouse_check_button_pressed(mb_right) &amp;&amp; !wait)
{
    lightOn = false;
    wait = true;
}
if (!lightOn &amp;&amp; mouse_check_button_pressed(mb_right) &amp;&amp; !wait)
